{
  "expo": {
    "name": "petGeo",
    "slug": "petGeo",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "height": 50,
      "weight": 50,
      "backgroundColor": "#FFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.brayhanjjuanz.petgeocinco",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCyBPXGoYlIZSE5A2a6BmjlLJ442KAI6R0"
        }
      },
      "permissions": [
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "description": "Aplicación móvil hecha en react native para la geolocalización y administración de información de mascotas",
    "githubUrl": "https://github.com/BrayhanJC/petGeo5"
  }
}
