      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.brayhanjjuanz.petgeocinco",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBcaEjskn_rHK1l2jak10_BPp9Pa3qT2rg"
        },
        "googleSignIn":{
          "apiKey": "AIzaSyBcaEjskn_rHK1l2jak10_BPp9Pa3qT2rg"
        }
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
